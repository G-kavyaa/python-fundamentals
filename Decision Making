#Budget
br=int(input())
vs=int(input())
s=int(input())
a=int(input())
l=int(input())
brc=br*(350.34)
vsc=vs*(230.90)
sc=s*(190.55)
ac=a*(125.30)
lc=l*(180.90)
total=(brc+vsc+sc+ac+lc)
if(total<15000):
    print("yes")
else:
    print("No")

#Sece Dining
D=str(input())
R=int(input())
if(D=='front'):
    if(R==1):
        print("Left Handed")
    else:
        print("Right Handed")
else:
    if (D=='rear'and R==1 ):
        print("Right Handed")
    else:
        print("Left Handed")
#Intersection of a Circle
X=int(input ())
Y=int(input())
r=int(input())
X1=int(input())
Y1=int(input())
r1=int(input())
d=((X-X1)**2 + (Y-Y1)**2)**(1/2)
if d < r+r1:
    print("The circles overlap")
elif d == r+r1:
    print("The circles are tangential")
else:
    print("Thr circles do not overlap")

#Microwave Oven
n=int(input("Enter the number of items:"))   
t=float(input("Enter the single item heating time:"))
if n==2:
    r=t+(0.5)*t
    s="{:.2f}".format(r)
    print("The recommended heating time is",s)
elif n==3:
    r=2*(t)
    print("The recommended heating time is",r)
elif n>3: 
    print("Number of items is more")
else:
    print("Invalid Input")
 
